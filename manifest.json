{
  "name": "Calimoto Enhancer",
  "version": "1.0.1",
  "description": "Add some welcome changes to Calimoto web Trip Planner",
  "author": "Jack NUMBER",
  "manifest_version": 2,
  "icons": {
    "512": "src/images/icon512.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "src/images/icon32.png"
    },
    "default_popup": "src/options/options.html"
  },
  "permissions": [
    "storage",
    "https://calimoto.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "https://calimoto.com/*"
    ],
    "js": [
      "src/main.js",
      "src/scripts/custom_css.js",
      "src/scripts/waypoints.js",
      "src/injector.js"
    ]
  }],
  "web_accessible_resources": [
    "src/inject_console_listener.js"
  ]
}
